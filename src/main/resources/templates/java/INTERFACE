package ${typesPackage};

<#list importPackages as i>
import ${i}.*;
</#list>

@SuppressWarnings({"all", "PMD"})
public interface ${name}
{
	<#list fields as f>
	${f.type.full} get${f.name?cap_first}();

	${useChainedAccessors?string(name,"void")} set${f.name?cap_first}(${f.type.full} v);

	</#list>
}
//CHECKSTYLE:ON
